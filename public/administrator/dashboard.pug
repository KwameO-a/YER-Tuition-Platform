  script.
    $('[data-toggle="datepicker"]').datepicker({
      inline : true,
      startDate : 'today',
      daysMin : ["Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat"],
      container : '.calendar-container',
      format : "DD MMM, YYYY",
      weekStart : 0
    }).on('pick.datepicker', function (e) {
      $('.full-date').text(e.date.toDateString());
      var classes = localStorage.classes ? JSON.parse(localStorage.classes) : [];
      $('.classes-for-day').empty();
      classes.records.forEach(element => {
        if(moment(element.fields["Start Date"]).isSame(e.date, 'day')) {
          let selectedCourse = [];
          var courses = localStorage.courses.records  ? JSON.parse(localStorage.courses) : [];
          selectedCourse = courses.filter((c, i) => { return c.id === element.fields.Course[0]; });
          $('.classes-for-day').append($(`<a href="/student/classroom" data-class-info='${JSON.stringify(element)}' class="course-block w-inline-block">
          <div class="course-header"></div>
          <div class="course-info-wrapper">
            <div class="div-block-5">
              <div class="course-title">${element.fields["Class Title"]}</div>
              <div class="course-blurb">${element.fields["Description"]}</div>
            </div>
            <div class="course-price-block w-clearfix">
              <div class="course-price mini">£${selectedCourse[0].fields.Price}</div>
              <div class="quoter mini">/ class</div>
            </div>
          </div>
        </a>`));
        }
      });
      $('a.course-block').on("click", function(e) {
        var classInfo = JSON.parse($(e.target).attr('data-class-info'));
        var courses = localStorage.courses ? JSON.parse(localStorage.courses) : [];
        var selectedCourse = courses.filter((c, i) => { return c.id === classInfo.fields.Course[0];})
        localStorage.setItem("selectedClass", JSON.stringify(classInfo));
        localStorage.setItem("selectedCourse", JSON.stringify(selectedCourse[0]));
      });
    });
    
